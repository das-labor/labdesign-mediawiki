{
	"name": "Labor",
	"namemsg": "skinname-labor",
	"author": [
		"Alex Legler and Gentoo wiki project",
		"Marcus Brinkmann",
		"fridtjof"
	],
	"url": "https://wiki.das-labor.org/",
	"descriptionmsg": "labor-desc",
	"license-name": "GPLv2",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"ValidSkinNames": {
		"labor": {
			"class": "SkinLabor",
			"args": [
				{
					"name": "labor",
					"templateDirectory": "skins/Labor/templates/",
					"template": "skin",
					"styles": [
						"mediawiki.skinning.interface",
						"mediawiki.skinning.content.externallinks",
						"skins.labor.styles"
					]
				}
			]
		}
	},
	"MessagesDirs": {
		"Labor": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"SkinMustache35Polyfill": "SkinMustache35Polyfill.php",
		"SkinMustache38Polyfill": "SkinMustache38Polyfill.php",
		"SkinLabor": "SkinLabor.php"
	},
	"ResourceModules": {
		"skins.labor.styles": {
			"position": "top",
			"styles": {
				"main.css": {
					"media": "screen"
				},
				"css/bootstrap.min.css": {},
				"css/labor.css": {},
				"css/flatpickr/flatpickr.min.css": {}
			}
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Labor"
	},
	"manifest_version": 2
}
